package ruless;
import com.curiosity.domain.Student
# 测试内置方法
rule "student"
    when
        $student:Student(age<10)
    then
        $student.setAge(15);
        System.out.println("触发了");
end
# 测试内置方法
rule "student2"
    when
        $student:Student(age>10 &&age<20)
    then
        System.out.println("触发了>10<15");
end
